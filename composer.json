{
    "name": "DavidVerdier/web-starter-kit",
    "description": "this is not a framework just a web starter kit built in PHP to work quickly",
    "license": "mit",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
